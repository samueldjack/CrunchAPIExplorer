<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Converters="clr-namespace:CrunchApiExplorer.Framework.MVVM.Converters">
    <Converters:NegatingConverter x:Key="Converter_Negating"/>
    <BooleanToVisibilityConverter x:Key="Converter_BooleanToVisibility"/>
    <Converters:BooleanToAnythingConverter x:Key="Converter_CollapsedWhenTrue" ValueWhenFalse="{x:Static Visibility.Visible}" ValueWhenTrue="{x:Static Visibility.Collapsed}"/>
    <Converters:TrueWhenStringNonEmptyConverter x:Key="Converter_TrueWhenNonEmpty"/>
</ResourceDictionary>